extends layout

block content


	div(ng-app="consulting")
		div(ng-controller="CompetencyProfileController", class="container-fluid")
			.row
				.col-md-12
					h1 Objectives
			.row
				.col-md-12
					#objectiveBox
						{{curr["Reviewed Objective"]}}
						input(type="checkbox", ng-model="curr.isMet")
			.row
				.col-md-6 
					#supportingExample.outline
						{{curr["Supporting Example"]}}
				.col-md-6						
					#counterExample.outline
						{{curr["Counter Example"]}}
			.row
				.col-md-4.col-md-offset-3
					button(ng-click="prev()") prev
					button(ng-click="next()") next
			.row
					{{currIndex+1}} / {{objectives.length}}
					input(type=text, ng-model="currIndex")
//					textarea(cols=200, rows=30) {{objectives|json}}
