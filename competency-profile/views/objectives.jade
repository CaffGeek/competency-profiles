.container-fluid
	.row
		.col-md-10.col-md-offset-1
			#objectiveFrame
			.row
				.col-md-8
					h1 Objectives
					h2 Met: <img src="../images/checkmark.png" height="35px" ng-show="vm.curr.isMet" alt="scored">
			.row
				.col-xs-4.navleft
					.hidden-xs.hidden-sm
						#progress
							|{{vm.currIndex+1}} / {{vm.objectives.length}}
					button(class="btn glyphicon glyphicon-chevron-left", ng-click="vm.prev()", ng-disabled="vm.currIndex <= 0") 
				.col-xs-4
					#progress
						pagination(ng-change="vm.pageChanged()" total-items="vm.objectives.length" ng-model="vm.pageNumber" class="pagination-sm" boundary-links="true" max-size="5" items-per-page="1")
				.col-xs-4.navright
					button(class="btn glyphicon glyphicon-chevron-right", ng-click="vm.next()" ng-disabled="vm.currIndex >= vm.objectives.length-1") 
			.row
				.col-md-12(ng-click="vm.meetObjective()" ng-class="(vm.curr.isMet) ? 'objectiveSelected' : 'objective'")
					#objectiveBox
						h2 Description
						#instructionText
							em click to score objective					
						#descriptionText.p
							|{{vm.curr.description}}
						.row
							.col-md-6
								#supportingExample
									h2 Supporting Example
									|{{vm.curr.supporting_example}}
							.col-md-6
								#counterExample
									h2 Counter Example
									|{{vm.curr.counter_example}}
					#earMarked(ng-show="(vm.curr.isMet)"  ng-class="(vm.curr.isMet) ? 'earMarked' : ''" )